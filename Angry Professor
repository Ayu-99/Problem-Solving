#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    
    int tt;
    scanf("%d",&tt);
    while (tt--)
    {
        int n,k;
        scanf("%d %d",&n,&k);
        while (n--)
        {
            int t;
            scanf("%d",&t);
            if (t<=0)
                k--;
        }
        if (k<=0)
            printf("NO\n");
        else
            printf("YES\n");
    }
    return 0;
}
