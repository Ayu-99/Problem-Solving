#include<iostream>
using  namespace std;
#include<bits/stdc++.h>


int main()
{
    int a[3];
    int b[3];
    for(int i=0;i<3;i++)
    {
    cin>>a[i];
    }
    for(int i=0;i<3;i++)
    {
    cin>>b[i];
    }
    int c=0,d=0,i;
    for(i=0;i<3;i++)
    {
        if(a[i]>b[i])
        {
            c=c+1;

        }
        else if(a[i]<b[i])
        {
            d=d+1;
        }
        else
        {
            continue;
        }
   }
   int g[2]={c,d};
   for(int i=0;i<2;i++)
    {
    cout<<g[i]<<" ";
    }

  return 0;
}

